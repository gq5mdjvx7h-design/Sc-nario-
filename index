];

function run(data, scenarios){
 return scenarios.filter(s=>s.test(data)).map(s=>s);
}

const list=[
 ...run(MONTHLY_DATA,MONTHLY_SCENARIOS),
 ...run(ANNUAL_DATA,ANNUAL_SCENARIOS)
];

const root=document.getElementById("scenarioList");
list.forEach(s=>{
 const d=document.createElement("div");
 d.className="scenario-card "+s.level;
 const data = s.title.includes("annuel")
  ? ANNUAL_DATA
  : MONTHLY_DATA;

d.innerHTML = `
  <strong>${s.title}</strong><br>
  ${s.message(data)}<br>
  <small>${s.legal}</small>
`;
 root.appendChild(d);
});
function reload() {
  const m = Number(document.getElementById("mhs").value);
  const a = Number(document.getElementById("ahs").value);

  MONTHLY_DATA.totalHS = m;
  ANNUAL_DATA.yearTotal = a;

  document.getElementById("scenarioList").innerHTML = "";
  const list = [
    ...run(MONTHLY_DATA, MONTHLY_SCENARIOS),
    ...run(ANNUAL_DATA, ANNUAL_SCENARIOS)
  ];

  list.forEach(s => {
    const d = document.createElement("div");
    d.className = "scenario-card " + s.level;
    const data = s.title.includes("annuel") ? ANNUAL_DATA : MONTHLY_DATA;
    d.innerHTML = `
      <strong>${s.title}</strong><br>
      ${s.message(data)}<br>
      <small>${s.legal}</small>
    `;
    scenarioList.appendChild(d);
  });
}
</script>
</body>
</html>
