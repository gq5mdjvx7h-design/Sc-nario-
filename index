<!-- Zone scénarios -->
<main id="scenarioList"></main>

<script src="scenarios.js"></script>

<script>
/* ======================
   NAVIGATION
====================== */
function goMenu(){
  window.location.href = "../menu.html";
}

/* ======================
   MOCK LECTURE DONNÉES
   (branchable sur tes modules)
====================== */
function readMonthlyData(){
  return window.MONTHLY_DATA || {};
}

function readAnnualData(){
  return window.ANNUAL_DATA || {};
}

/* ======================
   ÉVALUATION + RENDER
====================== */
function evaluateScenarios(){
  const monthly = readMonthlyData();
  const annual  = readAnnualData();

  // Fonction exposée par ton JS des 200 scénarios
  return window.evaluateAllScenarios(monthly, annual);
}

function renderScenarios(list){
  const container = document.getElementById("scenarioList");
  container.innerHTML = "";

  if(!list.length){
    container.innerHTML =
      `<p class="empty">Aucun scénario actif actuellement.</p>`;
    return;
  }

  list.forEach(s => {
    const card = document.createElement("div");
    card.className = `scenario-card ${s.level}`;
    card.dataset.type = s.type;

    card.innerHTML = `
      <div class="scenario-header">
        <span class="icon">${s.icon}</span>
        <span class="type">${s.typeLabel}</span>
      </div>

      <div class="scenario-title">${s.title}</div>

      <div class="scenario-body">
        ${s.message}
      </div>

      <div class="scenario-meta">
        ${s.legal || ""}
      </div>
    `;

    container.appendChild(card);
  });
}

/* ======================
   FILTRES
====================== */
document.querySelectorAll(".filters button").forEach(btn=>{
  btn.onclick = ()=>{
    document.querySelectorAll(".filters button")
      .forEach(b=>b.classList.remove("active"));
    btn.classList.add("active");

    const filter = btn.dataset.filter;
    document.querySelectorAll(".scenario-card").forEach(card=>{
      card.style.display =
        filter === "all" || card.dataset.type === filter
          ? "block"
          : "none";
    });
  };
});

/* ======================
   INIT
====================== */
document.addEventListener("DOMContentLoaded", ()=>{
  const scenarios = evaluateScenarios();
  renderScenarios(scenarios);
});
/* scenarios.full.js
   200 scénarios explicites
   100 Paie mensuelle / 100 Compteur annuel
   Conforme droit du travail FR
*/

// ===== MOTEUR =====
export function runScenarios(data, scenarios) {
  const out = [];
  for (const s of scenarios) {
    try {
      if (s.test(data)) {
        out.push({
          id: s.id,
          level: s.level,
          title: s.title,
          message: s.message(data),
          legal: s.legal
        });
      }
    } catch (e) {}
  }
  return out;
}

